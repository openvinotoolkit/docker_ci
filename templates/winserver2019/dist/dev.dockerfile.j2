# dev package
WORKDIR ${INTEL_OPENVINO_DIR}
ARG OPENVINO_WHEELS_URL
RUN IF not defined OPENVINO_WHEELS_URL ( `
        python -m pip install --no-cache-dir openvino=={{ product_version }} && `
        python -m pip install --no-cache-dir openvino_dev=={{ product_version }} `
    ) ELSE ( `
        python -m pip install --no-cache-dir openvino=={{ product_version }} --trusted-host=* --find-links %OPENVINO_WHEELS_URL% && `
        python -m pip install --no-cache-dir openvino_dev=={{ product_version }} --trusted-host=* --find-links %OPENVINO_WHEELS_URL% `
    )
